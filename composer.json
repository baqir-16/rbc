{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "7.1.*",
        "barryvdh/laravel-snappy": "^0.4.0",
        "doctrine/dbal": "^2.7",
        "guzzlehttp/guzzle": "^6.3",
        "h4cc/wkhtmltopdf-amd64": "0.12.2.1",
        "intervention/image": "^2.4",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "laravelcollective/remote": "^5.4.0",
        "maatwebsite/excel": "~2.1.0",
        "moloquent/moloquent": "dev-master",
        "mongodb/mongodb": "1.1.*",
        "paulvl/backup": "3.*",
        "phpseclib/phpseclib": "^2.0",
        "spatie/laravel-directory-cleanup": "^1.0",
        "spatie/laravel-permission": "^2.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "pcinaglia\\laraupdater\\": "packages/pcinaglia/laraupdater/src",
            "langleyfoxall\\passwordrules\\": "packages/langleyfoxall/passwordrules/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
